a  = input()

gls = 0
gls_a  = 0
gls_e  = 0
gls_i  = 0
gls_o  = 0
gls_u  = 0
sgls = 0

for i in range(len(a)):
    if a[i] in "aeiou":
        gls += 1
    else:
        sgls +=1
for i in range(len(a)):
    if a[i] in "a":
        gls_a +=1
for i in range(len(a)):
    if a[i] in "e":
        gls_e +=1
for i in range(len(a)):
    if a[i] in "i":
        gls_i +=1
for i in range(len(a)):
    if a[i] in "o":
        gls_o +=1
for i in range(len(a)):
    if a[i] in "u":
        gls_u +=1

print("Гласных букв =", gls)
print("Согласных букв =", sgls)
print("Гласных и согласных букв =", gls+sgls)
print("a = ", gls_a)
print("e = ", gls_e)
print("i = ", gls_i)
print("o = ", gls_o)
print("u = ", gls_u)
